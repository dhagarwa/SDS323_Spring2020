library(mosaic)
library(tidyverse)
library("ggplot2")
#Reading the data
gb = read.csv('../data/greenbuildings.csv')
summary(gb)
str(gb)
quantile(gb$leasing_rate, probs=c(0.01, 0.05, 0.1, 0.15, 0.2)) # to get an idea of quantiles
